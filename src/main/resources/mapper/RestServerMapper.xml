<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.restserver.mapper.RestServerMapper">

    <resultMap id="BaseResultMap" type="com.example.restserver.entity.RestServer">
        <id property="id" column="id" jdbcType="OTHER"/>
        <result property="createDate" column="create_date" jdbcType="DATE"/>
        <result property="updateDate" column="update_date" jdbcType="DATE"/>
        <result property="args" column="args" jdbcType="VARCHAR"/>
        <result property="data" column="data" jdbcType="VARCHAR"/>
        <result property="dataType" column="data_type" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        id
        ,create_date,update_date,
        args,`data`,data_type
    </sql>
</mapper>
